<!doctype html>
<html lang="zxx">

<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/bootstrap.min.css">
	<!-- Owl Theme Default CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/owl.theme.default.min.css">
	<!-- Owl Carousel CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/owl.carousel.min.css">
	<!-- Animate CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/animate.min.css">
	<!-- Flaticon CSS -->
	<link rel="stylesheet" href="/css/flaticon.css">
	<!-- Font Awesome CSS -->
	<link rel="stylesheet" href="/css/font-awesome.min.css">
	<!-- Nice Select CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/nice-select.css">
	<!-- Imagelightbox CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/imagelightbox.min.css">
	<!-- Meanmenu CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/meanmenu.min.css">
	<!-- Date Picker CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/date-picker.min.css">
	<!-- Date Picker CSS -->
	<link rel="stylesheet" href="https://d6fhaebhrj3za.cloudfront.net/css/odometer.min.css">
	<!-- Style CSS -->
	<link rel="stylesheet" href="/css/style.css">
	<!-- Responsive CSS -->
	<link rel="stylesheet" href="/css/responsive.css">

	<!-- Favicon -->
	<link rel="apple-touch-icon" sizes="180x180" href="/img//apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="/img/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/img/favicon-16x16.png">
	<link rel="manifest" href="/img/site.webmanifest">
	<!-- TITLE -->
	<title>Asahi - Food & Restaurant HTML Template</title>
</head>

<body>
	<!-- Start Preloader Area -->
	<div class="preloader">
		<div class="spinner">
			<div class="double-bounce1"></div>
			<div class="double-bounce2"></div>
		</div>
	</div>
	<!-- End Preloader Area -->

	<!-- Start Sign-up Area -->
	<section class="sign-up-area">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-6 col-md-6 p-0">
					<div class="sign-up-bg"></div>
				</div>
				<div class="col-lg-6 col-md-6 p-0">
					<% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error) { %>
					<div role="alert">
						<%= error.msg %>
						<button type="button" data-dismiss="alert" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<% }); %> <% } %> <% if(success_msg != ''){ %>
					<div class="alert alert-success alert-dismissible fade show" role="alert">
						<%= success_msg %>
						<button type="button" class="close" data-dismiss="alert" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<% } %> <% if(error_msg != ''){ %>
					<div class="alert alert-danger alert-dismissible fade show" role="alert">
						<%= error_msg %>
						<button type="button" class="close" data-dismiss="alert" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<% } %> <% if(error != ''){ %>
					<div class="alert alert-danger alert-dismissible fade show" role="alert">
						<%= error %>
						<button type="button" class="close" data-dismiss="alert" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<% } %>
					<div class="sign-up-form">
						<form action="/users/getuserAddress" method="GET">
							<button type="submit" class="btn default-btn">
								Fetch Address
							</button>
						</form>

						<h3>Add address</h3>
						<form action="/users/addAddress" method="POST">
							<div class="row">
								<div class="col-lg-12">
									<div class="form-group">
										<label>Email</label>
										<input type="email" name="address[email]" class="form-control"
											value=<%= user.email %> placeholder="Email Address" required>
									</div>
								</div>
								<div class="col-lg-12">
									<div class="form-group">
										<label>Full Address</label>
										<input type="text" name="address[fullAddress]" class="form-control"
											placeholder="Full Address" required>
									</div>
								</div>
								<div class="col-lg-12">
									<div class="form-group">
										<label>Phone</label>
										<input name="address[phone]" type="number" class="form-control" required
											placeholder="Phone Number">
									</div>
								</div>
								<div class="col-lg-6">
									<div class="form-group">
										<label>Landmark</label>
										<input name="address[landmark]" type="text" class="form-control"
											placeholder="Landmark">
									</div>
								</div>
								<div class="col-lg-6">
									<div class="form-group">
										<label>Locality</label>
										<input name="address[locality]" type="text" class="form-control"
											placeholder="Locality">
									</div>
								</div>
								<div class="col-lg-6">
									<div class="form-group">
										<label>Pincode</label>
										<input name="address[pincode]" type="number" required class="form-control"
											placeholder="Pincode Number">
									</div>
								</div>
								<div class="col-lg-6">
									<div class="form-group">
										<label>City</label>
										<input name="address[city]" type="text" required class="form-control"
											placeholder="City">
									</div>
								</div>
								<div class="col-lg-6">
									<div class="form-group">
										<label>State</label>
										<input name="address[state]" type="text" required class="form-control"
											placeholder="State">
									</div>
								</div>
								<div class="col-lg-6">
									<div class="form-group">
										<label>Country</label>
										<input name="address[country]" type="text" required class="form-control"
											placeholder="Country">
									</div>
								</div>
							</div>

							<div class="row">
								<div class="col">
									<p style="margin: 10px">
										<input required name="address[addressType]" type="radio" id="office"
											value="office">
										<label for="office">Office</label>
									</p>
								</div>

								<div class="col">
									<p style="margin: 10px">
										<input required name="address[addressType]" type="radio" id="home" value="home">
										<label for="home">Home</label>
									</p>
								</div>
								<div class="col">
									<p style="margin: 10px">
										<input required name="address[addressType]" type="radio" id="other"
											value="other">
										<label for="other">Other</label>
									</p>
								</div>
							</div>

							<div class="col-md-6 offset-md-3">
								<button type="submit" class="btn default-btn">
									Add Address
								</button>
							</div>
							<div class="col-lg-12">
								<p>
									Not User?
									<a href="/users/register">Register Again</a>
								</p>
								<p>

									<a href="/users/logout">Logout</a>
								</p>
							</div>
							<div class="col-lg-12">
								<ul class="social-list">
									<li>
										<a href="#">
											<i class="fa fa-facebook"></i>
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-twitter"></i>
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-google-plus"></i>
										</a>
									</li>
									<li>
										<a href="#">
											<i class="fa fa-linkedin"></i>
										</a>
									</li>
								</ul>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End Sign-up Area -->


	<!-- Jquery Slim JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/jquery-3.2.1.slim.min.js"></script>
	<!-- Popper JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/popper.min.js"></script>
	<!-- Bootstrap JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/bootstrap.min.js"></script>
	<!-- Meanmenu JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/jquery.meanmenu.js"></script>
	<!-- Nice Select JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/jquery.nice-select.min.js"></script>
	<!-- Wow JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/wow.min.js"></script>
	<!-- Owl Carousel JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/owl.carousel.js"></script>
	<!-- Bootstrap Datepicker JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/bootstrap-datepicker.min.js"></script>
	<!-- Imagelightbox JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/imagelightbox.min.js"></script>
	<!-- Appear JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/jquery.appear.js"></script>
	<!-- Odometer JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/odometer.min.js"></script>
	<!-- Form Ajaxchimp JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/jquery.ajaxchimp.min.js"></script>
	<!-- Form Validator JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/form-validator.min.js"></script>
	<!-- Contact Form JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/contact-form-script.js"></script>
	<!-- Custom JS -->
	<script src="https://d6fhaebhrj3za.cloudfront.net/js/custom.js"></script>
</body>

</html>