<%- include("partials/newHeader.ejs") %>
<style>
	#form {
        width: 60%;
    }
    @media (max-width: 600px) {
        #form {
            width: 100%;
        }
    }
</style>

<div data-content-region="header_bottom"></div>
<div class="body" data-currency-code="INR">

	<div class="home-content">

		<div class="tt-breadcrumbs-main">
			<div class="tt-breadcrumbs">
				<div class="container">
					<h1 class="category-name">
						<span class="breadcrumb-title ">
						</span>
						<span class="breadcrumb-title is-active">
							<span class="breadcrumb-label">Become a Franchisee</span>
						</span>
					</h1>
					<ul class="breadcrumbs" itemscope itemtype="http://schema.org/BreadcrumbList">
						<li class="breadcrumb " itemprop="itemListElement" itemscope
							itemtype="http://schema.org/ListItem">
							<a href="https://theme-payeeshop.mybigcommerce.com/" class="breadcrumb-label"
								itemprop="item"><span itemprop="name">Home</span></a>
							<meta itemprop="position" content="1" />
						</li>
						<li class="breadcrumb is-active" itemprop="itemListElement" itemscope
							itemtype="http://schema.org/ListItem">
							<meta itemprop="item" content="https://theme-payeeshop.mybigcommerce.com/contact-us/">
							<span class="breadcrumb-label" itemprop="name">Become a Franchisee</span>
							<meta itemprop="position" content="2" />
						</li>
					</ul>
				</div>
			</div>
		</div>

		<div class="tt-heroCarousel">
			<section class="heroCarousel" data-slick='{
			"arrows": true,
			"mobileFirst": true,
			"slidesToShow": 1,
			"fade": true,
			"slidesToScroll": 1,
			"autoplay": true,
			"autoplaySpeed": 5000,
			"lazyLoad": "anticipated"
		}'>
	
				<% var bannerArray = new Array() %>
				<% bannerArray.push({src: "https://d6fhaebhrj3za.cloudfront.net/images/franchise.jpg", linkTo: ""}) %>
	
				<% bannerArray.forEach((banner, index) => { %>
				<a href="<%=banner.linkTo%>" id="slider-slider<%=index%>">
					<div class="heroCarousel-slide">
						<img class="heroCarousel-image" src="<%=banner.src%>" alt="" />
					</div>
				</a>
				<% }) %>
			</section>
		</div>


		<main class="tt-page page-content--centered">
			<h1 class="page-heading">Become a Franchisee</h1>


			<div id="contact-us-page" class="page-content">
				<p>
				</p>
				<div style="text-align: center;">
					<iframe id="form"
						src="https://docs.google.com/forms/d/e/1FAIpQLSeXNoF1DSGurZcnQOvtzfLfpSh4L33zSVFvLtMsNsPzuWqGXA/viewform?embedded=true"
						height="943" frameborder="0" marginheight="0" marginwidth="0">Loadingâ€¦</iframe>
				</div>

			</div>

		</main>


	</div>
	<div id="modal" class="modal" data-reveal data-prevent-quick-search-close>
		<a href="#" class="modal-close" aria-label="Close" role="button">
			<span aria-hidden="true">&#215;</span>
		</a>
		<div class="modal-content"></div>
		<div class="loadingOverlay"></div>
	</div>
	<div id="alert-modal" class="modal modal--alert modal--small" data-reveal data-prevent-quick-search-close>
		<div class="swal2-icon swal2-error swal2-icon-show"><span class="swal2-x-mark"><span
					class="swal2-x-mark-line-left"></span><span class="swal2-x-mark-line-right"></span></span></div>

		<div class="modal-content"></div>

		<div class="button-container"><button type="button" class="confirm button" data-reveal-close>OK</button></div>
	</div>
</div>

<%- include("partials/newFooter.ejs") %>