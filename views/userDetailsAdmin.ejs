<%- include('partials/adminHeader'); -%>

<div class="content-wrapper">
    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Dashboard</a>
            </li>
            <li class="breadcrumb-item active">User Details</li>
        </ol>
        <div class="box_general padding_bottom">
            <div class="header_box version_2">
                <h2><i class="fa fa-file"></i>User Details</h2>
            </div>
            <div class="container">
                <br>
                <h1>User Details</h1>
                <br>
                <br>
                <h3>User ID: <%=user.uuid%></h3>
                <h3>Name: <%=user.name%></h3>
                <h3>Username: <%=user.username%></h3>
                <h3>IsAdmin: <%=user.isAdmin%></h3>
                <h3>Active: <%=user.active%></h3>
                <h3>Phone: <%=user.phone%></h3>
                <h3>Date: <%=user.dateCreated.toLocaleDateString()%></h3>
                <h3>IsBusiness: <%=user.isBusiness%></h3>
                <h3>Credit Allowed: <%=user.creditAllowed%></h3>
            </div>
        </div>
    </div>
</div>

<%- include('partials/adminFooter'); -%>