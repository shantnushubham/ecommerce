<%- include('partials/adminHeader'); -%>

<div class="content-wrapper">
    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Dashboard</a>
            </li>
            <li class="breadcrumb-item active">Update COD Allow</li>
        </ol>
        <div class="box_general padding_bottom">
            <div class="header_box version_2">
                <h2><i class="fa fa-file"></i>Update COD ALlow</h2>
            </div>

            <div class="container">
                <br>
                <h1>Update COD Allow</h1>
                <br>
                <br>
                <form action="/codAllow/update" method="POST">
                    <div class="ui form">

                        <div class="field">
                            <label>Name</label>
                            <input type="text" placeholder="Name" name="name" value="<%=cod.name%>" readonly required>
                        </div>

                        <div class="field">
                            <label>From</label>
                            <input type="number" placeholder="From" name="from" value="<%=cod.from%>" required>
                        </div>
                    </div>
                    <button class="ui button" type="submit">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>

<%- include('partials/adminFooter'); -%>