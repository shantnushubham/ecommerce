	<script>
		$(".filters").click(function () {

			$header = $(this);
			$content = $header.next();
			$content.slideToggle(500, function () {
				$header.text(function () {
					return $content.is(":visible") ? "Collapse Filters" : "Expand Filters";
				});
			});

		})
	</script>

