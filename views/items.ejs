		<main>
			<!--/hero_in-->
			<div class="container margin_60_35">
				<div class="row">
					<aside class="col-lg-3" id="sidebar">
						<div id="filters_col">
							<a class="filters" style="font-weight: bold;">Collapse Filters</a>
							<div class="collapse show" id="collapseFilters">
								<form method="POST" action="/item/filter">
									<hr>
									<div class="filter_type">
										<h5>Categories</h5>
										<ul>
											<% category.forEach(element => { %>
											<li>
												<label>
													<input type="checkbox" name="category">&nbsp; <b><%=element%></b>
												</label>
											</li>
											<% }); %>
										</ul>
									</div>
									<hr>
									<div class="filter_type">
										<h5>Sub-Category</h5>
										<ul>
											<% subCategory.forEach(element => { %>
											<li>
												<label>
													<input type="checkbox" name="subCategory">&nbsp; <b><%=element%></b>
												</label>
											</li>
											<% }); %>
										</ul>
									</div>
									<hr>
									<div class="filter_type">
										<h5>Tags</h5>
										<ul>
											<% tag.forEach(element => { %>
											<li>
												<label>
													<input type="checkbox" name="tags">&nbsp; <b><%=element%></b>
												</label>
											</li>
											<% }); %>
										</ul>
									</div>
									<button class="default-btn" type="submit">Search</button>
								</form>
							</div>
							<!--/collapse -->
						</div>
						<!--/filters col-->
					</aside>
					<!-- /aside -->

					<div class="col-lg-9">
						<div class="row">
							<div class="container">
								<div class="row">
									<% for (let x = 0; x < itemlist.length; x++) { %>
									<div class="col-lg-6 col-md-6 col-sm-6">
										<div class="single-menu-item" style="background-color: white !important;">
											<div class="menu-img">
												<img src="<%=itemlist[x].image%>" alt="Shop">
												<div class="price">
													<span>₹ <%= itemlist[x].price %></span>
												</div>
											</div>
											<div class="item-text">
												<h3>
													<a href="#"><%= itemlist[x].name %></a>
												</h3>
												<p><%= itemlist[x].content%></p>
												<ul>
													<li>
														<a class="default-btn"
															href="/items/<%= itemlist[x].iid %>">Order Now</a>
													</li>
