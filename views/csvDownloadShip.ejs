<%- include("partials/adminHeader.ejs") %>

<div class="content-wrapper">
    <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="#">Dashboard</a>
            </li>
            <li class="breadcrumb-item active">Download Invoice</li>
        </ol>
        <div class="box_general padding_bottom">
            <div class="header_box version_2">
                <h2><i class="fa fa-file"></i>Download Invoice</h2>
            </div>

            <div class="container">
                <br>
                <h1>Download Invoice</h1>
                <br>
                <br>
                <form action="/downloads/invoice/shipment" method="POST">
                    <div class="ui form">
                        <div class="field">
                            <label>Start Date</label>
                            <input type="date" placeholder="Start Date" name="from" value="">
                        </div>
                        <div class="field">
                            <label>End Date</label>
                            <input type="date" placeholder="End Date" name="to" value="" required>
                        </div>
                        <div class="field">
                            <label>Select Shipment Status</label>
                            <select id="cars" name="shipment" required>
                                <option value="all">All</option>
                                <option value="processing">Processing</option>
                                <option value="approved">Approved</option>
                                <option value="cancelled">Cancelled</option>
                                <option value="completed">Completed</option>
                                <option value="cancellation-processing">Cancellation Processing</option>
                                <option value="saved">Saved</option>
                            </select>
                        </div>
                    </div>
                    <button class="ui button" type="submit">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>

<%- include("partials/adminFooter.ejs") %>